# ThingSpeak Configuration
# Get these values from your ThingSpeak channel

# Your ThingSpeak Channel ID
THINGSPEAK_CHANNEL_ID=your_channel_id_here

# Read API Key (for fetching data)
THINGSPEAK_READ_API_KEY=your_read_api_key_here

# Write API Key (for sending motor mode commands)
THINGSPEAK_WRITE_API_KEY=your_write_api_key_here

# Server Port (optional, defaults to 3000)
PORT=3000